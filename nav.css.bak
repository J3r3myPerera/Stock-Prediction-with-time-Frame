body{
    margin: 0;
    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
    background-image: url("Home page background.png");
    background-size: 100%;
}

#header{
    margin: 5px;
    border-radius: 5px;
    height: 10vh;
    width: 99%;
    float: left;
}

/* The Navigation Bar*/

#nav {
			
    overflow: hidden;
    background-color:rgb(rgb(37, 150, 190));
    width: 100%;
    padding: 1px;
    
}

#nav a {
    float: right;
    display: block;
    color: #f2f2f2;
    text-align: center;
    padding: 32px;
    text-decoration: none; 
    font-size: 20px;
}

#nav a:hover {
    background-color: #ddd;
    color: black;
}

#nav a.active {
    background-color: #04AA6D;
    color: white;
	radius : 10px
}

